@model WebApplication.Models.Home.Details
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<div>Id: @Model.Id</div>
<div>Name: @Model.Name</div>
<div>Version: @Model.Version</div>
<div>Current Count: @Model.CurrentCount</div>


<a asp-controller="Home" asp-action="ChangeName" asp-route-id="@Model.Id">Change Name</a>
<a asp-controller="Home" asp-action="CheckIn" asp-route-id="@Model.Id">Check In</a>
<a asp-controller="Home" asp-action="Deactivate" asp-route-id="@Model.Id">Deactivate</a>
<a asp-controller="Home" asp-action="Remove" asp-route-id="@Model.Id">Remove</a>


